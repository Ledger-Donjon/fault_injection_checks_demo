{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build fi_test",
			"command": "cargo",
			"args": ["build", "--release", "--example", "fi_test"],
		},
		{
			"label": "Fault: Safe PartialEq test",
			"command": "python",
			"args": ["fi_check.py", "fi_test_hard", "fi_test_hard2"],
			"problemMatcher": {
				"owner": "rust",
				"fileLocation": ["absolute"],
				"pattern": {
					"regexp": "warning: '(.*?)' (.*?):(\\d+):(\\d+)",
					"file": 2,
					"line": 3,
					"column": 4,
					"message": 1
				}
			},
			"dependsOn": ["build fi_test"]
		}
	],
}